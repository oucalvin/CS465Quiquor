<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?scenebuilder-stylesheet ../default.css?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="client.controller.Welcome">
  <children>
    <StackPane minHeight="300.0" minWidth="300.0" prefHeight="300.0" prefWidth="300.0" styleClass="dark-gradientpane">
      <children>
        <VBox prefHeight="200.0" prefWidth="100.0">
          <children>
            <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0">
              <children>
                <Button mnemonicParsing="false" text="Settings" textAlignment="LEFT" />
              </children>
            </HBox>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Quiquor">
              <font>
                <Font size="12.0" />
              </font>
            </Text>
            <Button mnemonicParsing="false" onAction="#gotoHome" text="Order a Drink!" />
          </children>
        </VBox>
      </children>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</GridPane>
