<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-stylesheet ../default.css?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/2.2" fx:controller="client.controller.Home">
    <children>
        <StackPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="580.0" prefWidth="900.0"
                   styleClass="dark-gradientpane">
            <children>
                <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="-1.0" prefWidth="900.0">
                    <children>
                        <AnchorPane id="AnchorPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="114.0"
                                    prefWidth="900.0">
                            <children>
                                <HBox fx:id="topHboxBar" alignment="TOP_RIGHT" layoutX="44.0" layoutY="0.0"
                                      prefHeight="100.0" prefWidth="474.0"/>
                                <Button layoutX="863.0" layoutY="14.0" mnemonicParsing="false"
                                        onAction="#gotoCreateYourOwnDrink" text="C"/>
                                <Text fill="WHITE" layoutX="726.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="Create Your Own Drink!"/>
                                <Text fx:id="drinkNameText" fill="WHITE" layoutX="539.0" layoutY="81.0"
                                      strokeType="OUTSIDE" strokeWidth="0.0" text="Some Drink">
                                    <font>
                                        <Font size="33.0"/>
                                    </font>
                                </Text>
                                <Button layoutX="8.0" layoutY="5.0" mnemonicParsing="false"
                                        onAction="#promptForPassword" text="S"/>
                            </children>
                        </AnchorPane>
                        <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="900.0">
                            <children>
                                <TextField fx:id="drinkSearchField" layoutX="50.0" layoutY="14.0" prefWidth="205.0"/>
                                <ListView fx:id="drinkListView" layoutX="50.0" layoutY="51.0" prefHeight="291.0"
                                          prefWidth="205.0"/>
                                <AnchorPane fx:id="guiControllerPane" layoutX="479.0" layoutY="0.0"
                                            minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
                                            prefWidth="400.0"/>
                                <Button layoutX="24.0" layoutY="72.0" mnemonicParsing="false" text="F"/>
                                <Button layoutX="22.0" layoutY="51.0" mnemonicParsing="false" text="A"/>
                                <Button layoutX="23.0" layoutY="93.0" mnemonicParsing="false" text="U"/>
                            </children>
                        </AnchorPane>
                        <AnchorPane prefHeight="35.0" prefWidth="940.0">
                            <children>
                                <Button layoutX="790.0" layoutY="11.0" mnemonicParsing="false" text="Pour My Drink!"/>
                            </children>
                        </AnchorPane>
                    </children>
                </VBox>
                <AnchorPane id="passwordModal" fx:id="modal" minHeight="-Infinity" minWidth="-Infinity"
                            prefHeight="600.0" prefWidth="920.0" styleClass="overlay" visible="false">
                    <children>
                        <AnchorPane layoutX="301.0" layoutY="176.0" prefHeight="208.0" prefWidth="338.0"
                                    styleClass="dark-gradientpane">
                            <children>
                                <PasswordField id="passwordField1" fx:id="password" layoutX="71.0" layoutY="82.0"
                                               prefWidth="200.0"/>
                                <Button layoutX="208.0" layoutY="117.0" mnemonicParsing="false" text="Confirm"/>
                                <Text fx:id="passwordStatus" fill="WHITE" layoutX="98.0" layoutY="132.0" scaleX="1.0"
                                      scaleY="0.9999923519334313" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="This is the status"/>
                                <Text fill="WHITE" layoutX="71.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                      text="Please enter a password to unlock...">
                                    <font>
                                        <Font name="System Italic" size="11.0"/>
                                    </font>
                                </Text>
                            </children>
                        </AnchorPane>
                    </children>
                </AnchorPane>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </StackPane>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
</GridPane>
